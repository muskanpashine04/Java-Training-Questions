import java.util.Scanner;

public class MethodsPractice {

    // 1. Write a program to create a method that prints "Java is Fun!".
    static void printJavaFun() {
        System.out.println("Java is Fun!");
    }

    // 2. Create a method and print your favorite programming language.
    static void printFavoriteLanguage() {
        System.out.println("Python");
    }

    // 3. Write a method that prints your name three times using only method calls.
    static void printNameThrice(String name) {
        System.out.println(name);
        System.out.println(name);
        System.out.println(name);
    }

    // 4. Create a method that prints "Welcome to Java Methods" twice without using loops.
    static void printWelcomeTwice() {
        System.out.println("Welcome to Java Methods");
        System.out.println("Welcome to Java Methods");
    }

    // 5. Design a method that prints your first name and last name in two separate lines.
    static void printFirstAndLastName(String firstName, String lastName) {
        System.out.println(firstName);
        System.out.println(lastName);
    }

    // 6. Write a method that receives a number and prints it twice without using loops.
    static void printNumberTwice(int number) {
        System.out.print(number);
        System.out.println(number);
    }

    // 7. Create a method that takes a string as input and prints that string.
    static void printString(String str) {
        System.out.println(str);
    }

    // 8. Write a program to create a method that takes two numbers and prints their difference.
    static void printDifference(int num1, int num2) {
        System.out.println(num1 - num2);
    }

    // 9. Define a method that accepts a decimal value and prints it.
    static void printDecimal(double value) {
        System.out.println(value);
    }

    // 10. Write a method that takes two city names and prints "Traveling from Bhopal to Indore".
    static void printTravelInfo(String city1, String city2) {
        System.out.println("Traveling from " + city1 + " to " + city2);
    }

    // 11. Create a method that returns the sum of two given numbers.
    static int sum(int num1, int num2) {
        return num1 + num2;
    }

    // 12. Write a method that receives a string and returns the same string with "Welcome " added before it.
    static String addWelcome(String str) {
        return "Welcome " + str;
    }

    // 13. Define a method that takes a number and returns its square.
    static int square(int number) {
        return number * number;
    }

    // 14. Write a method that receives two integers and returns their multiplication result.
    static int multiply(int num1, int num2) {
        return num1 * num2;
    }

    // 15. Implement a method that receives a number and returns the number increased by 10.
    static int increaseByTen(int number) {
        return number + 10;
    }

    // 16. Write a static method that prints "This is a static method".
    static void printStaticMessage() {
        System.out.println("This is a static method");
    }

    // 17. Create a static method that receives an integer and prints "The number is: 6". (Any Number)
    static void printTheNumber(int number) {
        System.out.println("The number is: " + number);
    }

    // 18. Write a static method that receives two numbers and returns their product.
    static int getProduct(int num1, int num2) {
        return num1 * num2;
    }

    // 19. Implement a static method that takes a name as an argument and returns "Hello, [name]!".
    static String getHelloName(String name) {
        return "Hello, " + name + "!";
    }

    // 20. Create a non-static method that prints "This is a non-static method".
    void printNonStaticMessage() {
        System.out.println("This is a non-static method");
    }

    // 21. Write a non-static method that receives a number and prints "The given number is [number]".
    void printGivenNumber(int number) {
        System.out.println("The given number is " + number);
    }

    // 22. Define a non-static method that takes two decimal values and prints their sum.
    void printDecimalSum(double num1, double num2) {
        System.out.println(num1 + num2);
    }

    // 23. Write a program that has two methods: one prints "Method without parameter", another prints "Method with parameter".
    static void methodWithoutParameter() {
        System.out.println("Method without parameter");
    }

    static void methodWithParameter() {
        System.out.println("Method with parameter");
    }

    // 24. Create a program where one method receives two numbers and returns their addition, and another method receives three numbers and prints their multiplication.
    static int addTwoNumbers(int a, int b) {
        return a + b;
    }

    static void multiplyThreeNumbers(int a, int b, int c) {
        System.out.println(a * b * c);
    }

     // 25. Write a class with two methods: one method prints "Hello", and another method takes a string and prints "Hello, [string]". (Any String you can take)
    static void printHello() {
        System.out.println("Hello");
    }

    static void printHelloString(String str) {
        System.out.println("Hello, " + str);
    }

    // 26. Implement two methods: one that prints "Java Programming", and another that takes a string and returns "Welcome to [string]".
    static void printJavaProgramming() {
        System.out.println("Java Programming");
    }

    static String getWelcomeString(String str) {
        return "Welcome to " + str;
    }

    // 27. Create a class named "Shop" with a method that receives a product name and price. The method should return a statement: "The price of [product] is [price]".
    static String getProductPriceStatement(String product, int price) {
        return "The price of " + product + " is " + price;
    }

     // 28. Write a program with a method that takes a person's first name and last name as separate inputs and returns a single full name.
    static String getFullName(String firstName, String lastName) {
        return firstName + " " + lastName;
    }

    // 29. Define a method that receives two numbers and prints the sum, multiplication, division, subtraction, and modulus of the numbers.
    static void printAllOperations(int num1, int num2) {
        System.out.println("Sum: " + (num1 + num2));
        System.out.println("Multiplication: " + (num1 * num2));
        if (num2 != 0) {
            System.out.println("Division: " + ((double) num1 / num2)); // Cast to double for accurate division
        } else {
            System.out.println("Division: Cannot divide by zero");
        }
        System.out.println("Subtraction: " + (num1 - num2));
        System.out.println("Modulus: " + (num1 % num2));
    }

    // 30. Create a class named "Operations" with a method that takes two integers and returns their average as a decimal value.
    static double getAverage(int num1, int num2) {
        return (double) (num1 + num2) / 2;
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // 1.
        printJavaFun();

        // 2.
        printFavoriteLanguage();

        // 3.
        printNameThrice("John");

        // 4.
        printWelcomeTwice();

        // 5.
        printFirstAndLastName("John", "Doe");

        // 6.
        printNumberTwice(5);

        // 7.
        printString("Hello World");

        // 8.
        printDifference(10, 4);

        // 9.
        printDecimal(3.14);

        // 10.
        printTravelInfo("Bhopal", "Indore");

        // 11.
        System.out.println(sum(5, 10));

        // 12.
        System.out.println(addWelcome("John"));

        // 13.
        System.out.println(square(5));

        // 14.
        System.out.println(multiply(5, 6));

        // 15.
        System.out.println(increaseByTen(15));

        // 16.
        printStaticMessage();

        // 17.
        printTheNumber(10);

        // 18.
        System.out.println(getProduct(5, 6));

        // 19.
        System.out.println(getHelloName("John"));

        // 20.
        MethodsPractice obj = new MethodsPractice();
        obj.printNonStaticMessage();

        // 21.
        obj.printGivenNumber(10);

        // 22.
        obj.printDecimalSum(3.5, 4.5);

        // 23.
        methodWithoutParameter();
        methodWithParameter();

        // 24.
        System.out.println(addTwoNumbers(5, 10));
        multiplyThreeNumbers(2, 3, 4);

        // 25.
        printHello();
        printHelloString("World");

        // 26.
        printJavaProgramming();
        System.out.println(getWelcomeString("Java"));

        // 27.
        System.out.println(getProductPriceStatement("Laptop", 50000));

        // 28.
        System.out.println(getFullName("John", "Doe"));

        // 29.
        printAllOperations(10, 5);

        // 30.
        System.out.println(getAverage(10, 20));

        scanner.close();
    }
}
